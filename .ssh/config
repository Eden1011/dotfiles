Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/github
    IdentitiesOnly yes
    AddKeysToAgent yes

Host gitlab.com
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/gitlab
    IdentitiesOnly yes
    AddKeysToAgent yes

Host *
    ServerAliveInterval 60
    ServerAliveCountMax 30
    TCPKeepAlive yes
    Compression yes
